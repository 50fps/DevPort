<h1>Homepage</h1>
<p>Find me in app/views/pages/home.html.erb</p>

<h4>BLogs:</h4>

<%= @posts.map do |post| %>
	<h3><%= post.title %></h3>
	<p><%= post.body %></p>
	<hr />
<% end %>

<h4>Skills:</h4>

<%= @skills.map do |post| %>
	<h3><%= post.title %></h3>
	<p><%= post.percent_utilized %></p>
	<hr />
<% end %>

