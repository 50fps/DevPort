<main role="main" class="inner cover text-center">
  <h1 class="cover-heading">Rails News</h1>

  <div class="row">
  	<% @tweets.collect do |tweet| %>
  		<div class="col-md-6">
  			<div class="tech-news">
  				<div class="card">
  					<%= tweet %>
  				</div>
  			</div>
  		</div>
  	<% end %>
  </div>
</main>